<h1>Edit Entry</h1>

<p>
<%= form_for @entry do |f| %>
<div>
<%= f.label :minutes %><br>
<%= f.number_field :minutes %>
</div>
<div>
<%= f.label :hours %><br>
<%= f.number_field :hours %>
</div>
<div>
<%= f.label :start_date %><br>
<%= f.date_field :start_date %>
</div>
<div>
<%= f.label :comment %><br>
<%= f.text_area :comment %>
</div>
<div>
<%= f.submit "Edit Entry" %>
</div>
<% end %>
</p>
